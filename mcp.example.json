{
  "mcpServers": {
    "mesimati-supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@0.4.5",
        "--project-ref=YOUR_PROJECT_REF_HERE"
      ],
      "env": {
